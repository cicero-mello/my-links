{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "paths": {
            "@colors": [
                "./src/styles/colors.ts"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "@components": [
                "./src/components/index.ts"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@storage": [
                "./src/storage/index.ts"
            ],
            "@page-styles/*": [
                "./src/styles/pages/*"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}
